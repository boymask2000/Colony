package com.colony.buildings.actors;

import com.badlogic.gdx.scenes.scene2d.Stage;
import com.colony.Draggable;
import com.colony.buildings.Anagrafica;
import com.colony.buildings.Castello;
import com.colony.enums.TipoElemento;

public class CastelloActor extends Draggable {

	
 

	public CastelloActor(float x, float y) {
		super(x, y);
	
		
	}
	public CastelloActor(float x, float y, Stage s) {
		super(x, y, s, TipoElemento.CASTELLO);
	
		edificio = Anagrafica.createCastello(x,y,s, this);
	
	
	
	}
	public void loadAnim() {
		String animations[] ={ "castello.png" };
		loadAnimationFromFiles(animations, 0.1f, true);
		
	}
	@Override
	protected void notifyEndDragging() {
		// TODO Auto-generated method stub
		
	}
}
