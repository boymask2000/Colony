package com.colony.buildings;

import com.badlogic.gdx.scenes.scene2d.Stage;
import com.colony.Draggable;

public class SeminaAlberi extends Draggable {
	private String animations[] ={ "boscaiolo.png" };

	public SeminaAlberi(float x, float y) {
		super(x, y);
	
		
	}
	public SeminaAlberi(float x, float y, Stage s) {
		super(x, y, s);
	
	
	
	}
	public void loadAnim() {
		loadAnimationFromFiles(animations, 0.1f, true);
		
	}
}
