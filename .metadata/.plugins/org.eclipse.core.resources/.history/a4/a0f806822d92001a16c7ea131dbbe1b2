package com.colony.fixed;

import com.badlogic.gdx.scenes.scene2d.Stage;
import com.colony.BaseActor;
import com.colony.enums.TipoElemento;

public class Albero extends BaseActor {
	public Albero(float x, float y, Stage s) {

		super(x, y, s, TipoElemento.ALBERO);

		String animations[] = { "tree.png" };
		loadAnimationFromFiles(animations, 0.1f, true);
	}

	@Override
	public void refreshEdificio() {
		
	}

}
