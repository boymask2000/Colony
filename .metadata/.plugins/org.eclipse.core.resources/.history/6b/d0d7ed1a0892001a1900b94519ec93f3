package com.colony.buildings.actors;

import com.badlogic.gdx.scenes.scene2d.Stage;
import com.colony.Draggable;
import com.colony.commons.Anagrafica;
import com.colony.enums.TipoElemento;

public class SegheriaActor extends Draggable {

	
 

	public SegheriaActor(float x, float y, Stage s) {
		super(x, y, s, TipoElemento.POZZO);

		edificio = Anagrafica.createPozzo(x,y,s, this);
	
	
	
	}
	public void loadAnim() {
		String animations[] ={ "pozzo.png" };
		loadAnimationFromFiles(animations, 0.1f, true);
		
	}
	@Override
	protected void notifyEndDragging() {
		// TODO Auto-generated method stub
		
	}
}
