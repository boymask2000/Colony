package com.colony.buildings.actors;

import com.badlogic.gdx.scenes.scene2d.Stage;
import com.colony.Draggable;
import com.colony.buildings.BuildingsHub;

public class EdificioSuAreaActor extends Draggable {

	
 

	public EdificioSuAreaActor(float x, float y) {
		super(x, y);
	
		
	}
	public EdificioSuAreaActor(float x, float y, Stage s) {
		super(x, y, s);

		edificio = BuildingsHub.createPozzo(x,y,s, this);
	
	
	
	}
	public void loadAnim() {
		String animations[] ={ "pozzo.png" };
		loadAnimationFromFiles(animations, 0.1f, true);
		
	}
}
