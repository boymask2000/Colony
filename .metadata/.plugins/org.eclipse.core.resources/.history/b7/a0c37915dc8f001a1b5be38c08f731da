package com.colony;

import com.badlogic.gdx.scenes.scene2d.Stage;

public class Carrier extends Draggable {

	public Carrier(float x, float y) {
		super(x, y);
	
		
	}
	public Carrier(float x, float y, Stage s) {
		super(x, y, s);
	
	
	
	}
	public void loadAnim() {
		String animations[] ={ "b.jpg" };
		loadAnimationFromFiles(animations, 0.1f, true);
		
	}

}
