package com.colony.buildings.actors;

import com.badlogic.gdx.scenes.scene2d.Stage;
import com.colony.Draggable;
import com.colony.commons.Anagrafica;
import com.colony.enums.TipoElemento;

public class TaglialegnaActor extends Draggable {
 

	public TaglialegnaActor(float x, float y) {
		super(x, y);
	
		
	}
	public TaglialegnaActor(float x, float y, Stage s) {
		super(x, y, s, TipoElemento.BOSCAIOLO);
	
		edificio = Anagrafica.createBoscaiolo(x,y,s, this);
	
	}
	public void loadAnim() {
		String animations[] ={ "boscaiolo.png" };
		loadAnimationFromFiles(animations, 0.1f, true);
		
	}
	@Override
	protected void notifyEndDragging() {
		// TODO Auto-generated method stub
		
	}
}
